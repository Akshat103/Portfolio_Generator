<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="<%= resumeData.name %>"> <title><%= resumeData.name %> - <%= resumeData.title %></title> <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:100,200,300,400,500,600,700,800,900" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i" rel="stylesheet">
  <link href="vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet">
  <link href="vendor/devicons/css/devicons.min.css" rel="stylesheet">
  <link href="vendor/simple-line-icons/css/simple-line-icons.css" rel="stylesheet">
  <link href="css/resume.min.css" rel="stylesheet">
</head>
<body id="page-top">
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
        <a class="navbar-brand js-scroll-trigger" href="#page-top">
          <span class="d-block d-lg-none"><%= resumeData.name %></span>
          <span class="d-none d-lg-block">
            <img class="img-fluid img-profile rounded-circle mx-auto mb-2" src="<%= resumeData.profileImage %>" alt="">
          </span>
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="#about">About</a>
            </li>
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="#education">Education</a>
            </li>
            </ul>
        </div>
      </nav>
      <div class="container-fluid p-0">      
        <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
            <div class="my-auto">
              <h1 class="mb-0"><%= resumeData.name %> <span class="text-primary"><%= resumeData.lastName %></span></h1>
              <div class="subheading mb-5"><%= resumeData.title %> <a href="mailto:<%= resumeData.email %>"><%= resumeData.email %></a></div>
              <p class="mb-5"><%= resumeData.description %></p>
              <p class="mb-5"><%= resumeData.additionalInfo %></p>
              <ul class="list-inline list-social-icons mb-0">
                <li class="list-inline-item">
                  <a href="<%= resumeData.socialLinks.twitter %>">
                    <span class="fa-stack fa-lg">
                      <i class="fa fa-circle fa-stack-2x"></i>
                      <i class="fa fa-twitter fa-stack-1x fa-inverse"></i>
                    </span>
                  </a>
                </li>
                <li class="list-inline-item">
                  <a href="<%= resumeData.socialLinks.linkedin %>">
                    <span class="fa-stack fa-lg">
                      <i class="fa fa-circle fa-stack-2x"></i>
                      <i class="fa fa-linkedin fa-stack-1x fa-inverse"></i>
                    </span>
                  </a>
                </li>
                <li class="list-inline-item">
                  <a href="<%= resumeData.socialLinks.github %>">
                    <span class="fa-stack fa-lg">
                      <i class="fa fa-circle fa-stack-2x"></i>
                      <i class="fa fa-github fa-stack-1x fa-inverse"></i>
                    </span>
                  </a>
                </li>
              </ul>
            </div>
          </section>
          <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="education">
            <div class="my-auto">
              <h2 class="mb-5">Education</h2>
              <% for (let i = 0; i < resumeData.education.length; i++) { %>
                <div class="resume-item d-flex flex-column flex-md-row mb-5">
                  <div class="resume-content mr-auto">
                    <h3 class="mb-0"><%= resumeData.education[i].institute %></h3>
                    <div class="subheading mb-3"><%= resumeData.education[i].degree %></div>
                    <% if (resumeData.education[i].duration) { %>
                      <div><%= resumeData.education[i].duration %></div>
                    <% } %>
                    <p><%= resumeData.education[i].grades %></p>
                  </div>
                  <% if (resumeData.education[i].dates) { %>
                    <div class="resume-date text-md-right"> <span class="text-primary"><%= resumeData.education[i].dates %></span> </div>
                  <% } %>
                </div>
              <% } %>
            </div>
          </section>    
          <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="project">
            <div class="my-auto">
              <h2 class="mb-5">Projects</h2>
              <% for (let i = 0; i < resumeData.projects.length; i++) { %>
                <div class="resume-item d-flex flex-column flex-md-row mb-5">
                  <div class="resume-content mr-auto">
                    <h3 class="mb-0"><a href="<%= resumeData.projects[i].link %>"><%= resumeData.projects[i].name %></a></h3>
                    <div class="subheading mb-3"><%= resumeData.projects[i].technologies %></div>
                    <p><%= resumeData.projects[i].description %></p>
                  </div>
                  <div class="resume-date text-md-right"> <span class="text-primary"><%= resumeData.projects[i].role %>, <%= resumeData.projects[i].date %></span> </div>
                </div>
              <% } %>
            </div>
          </section>
          <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="summary">
            <div class="my-auto">
              <h2 class="mb-5">Summary</h2>
              <p><%= resumeData.summary %></p>
            </div>
          </section>
          <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="awards">
            <div class="my-auto">
              <h2 class="mb-5">Awards &amp; Certifications</h2>
              <ul class="fa-ul mb-0">
                <% for (let i = 0; i < resumeData.awards.length; i++) { %>
                  <li>
                    <i class="fa-li fa fa-trophy text-warning"></i>
                    <%= resumeData.awards[i].name %>
                    <% if (resumeData.awards[i].link) { %>
                      <a href="<%= resumeData.awards[i].link %>" target="_blank"><i class="fa fa-link"></i> View Certificate</a>
                    <% } else { %>
                      <%-- Optionally display a placeholder or indicate "Link coming soon" --%>
                    <% } %>
                  </li>
                <% } %>
              </ul>
              
              <a target=”_blank” href="<%= resumeData.resumeLink %>"><button type="button" class="btn btn-dark resume">Resume</button></a>
            </div>
          </section>
        </div>
        <script src="vendor/jquery/jquery.min.js"></script>
        <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="vendor/jquery-easing/jquery.easing.min.js"></script>
        <script src="js/resume.min.js"></script>      
</body>
</html>        
